{
  "build": {
    "distDir": "../public",
    "withGlobalTauri": false,
    "devPath": "https://www.google.com/android/find/"
  },
  "package": {
    "productName": "qaftray",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": false
      }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.RelayGround.qaftray",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "qaftray",
        "skipTaskbar": true,
        "alwaysOnTop": true,
        "center": true,
        "width": 400,
        "height": 600
      }
    ]
  }
}